  <augment target-node="/protocols:protocols">
    <container name="ripng">
      <presence value="true"/>
      <configd:priority priority="660"/>
      <configd:help text="Routing Information Protocol (RIPng) parameters"/>
      <configd:create executable="/opt/vyatta/share/tmplscripts/protocols/ripng/configd_create.sh"/>
      <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/configd_delete.sh"/>
      <leaf-list name="aggregate-address">
        <type name="types:ipv6-prefix">
          <configd:syntax executable="/opt/vyatta/share/tmplscripts/protocols/ripng/aggregate-address/configd_syntax1.cli"/>
        </type>
        <ordered-by value="user"/>
        <configd:help text="Aggregate RIPng route announcement"/>
        <configd:create executable="/opt/vyatta/share/tmplscripts/protocols/ripng/aggregate-address/configd_create.sh"/>
        <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/aggregate-address/configd_delete.sh"/>
      </leaf-list>
      <leaf-list name="route">
        <type name="types:ipv6-prefix">
          <configd:syntax executable="/opt/vyatta/share/tmplscripts/protocols/ripng/route/configd_syntax1.cli"/>
        </type>
        <ordered-by value="user"/>
        <configd:help text="RIPng static route"/>
        <configd:create executable="/opt/vyatta/share/tmplscripts/protocols/ripng/route/configd_create.sh"/>
        <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/route/configd_delete.sh"/>
      </leaf-list>
      <container name="timers">
        <presence value="true"/>
        <configd:help text="RIPng timer values"/>
        <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/timers/configd_end.sh"/>
        <leaf name="garbage-collection">
          <type name="uint32">
            <range value="5..65535">
              <error-message>
                <value>Garbage collection timer must be between 5 and 65535</value>
              </error-message>
            </range>
          </type>
          <default value="120"/>
          <configd:help text="Garbage collection timer"/>
        </leaf>
        <leaf name="update">
          <type name="uint32">
            <range value="5..65535">
              <error-message>
                <value>Update timer must be between 5 and 65535</value>
              </error-message>
            </range>
          </type>
          <default value="30"/>
          <configd:help text="Routing table update timer"/>
        </leaf>
        <leaf name="timeout">
          <type name="uint32">
            <range value="5..65535">
              <error-message>
                <value>Timeout timer must be between 5 and 65535</value>
              </error-message>
            </range>
          </type>
          <default value="180"/>
          <configd:help text="Routing information timeout timer"/>
        </leaf>
      </container>
      <container name="distribute-list">
        <presence value="true"/>
        <configd:help text="Filter networks in routing updates"/>
        <container name="access-list">
          <presence value="true"/>
          <configd:help text="Access list"/>
          <leaf name="out">
            <type name="string">
              <pattern value="[!-~]{1,64}">
                <error-message>
                  <value>access-list name must be 64 characters or less</value>
                </error-message>
              </pattern>
              <pattern value="[^-].*">
                <error-message>
                  <value>access-list name cannot start with "-"</value>
                </error-message>
              </pattern>
              <pattern value="[^\|;&amp;$&lt;&gt;]*">
                <error-message>
                  <value>access-list name cannot contain shell punctuation</value>
                </error-message>
              </pattern>
            </type>
            <configd:help text="Access list to apply to output packets"/>
            <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/out/configd_update.sh"/>
            <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/out/configd_delete.sh"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/out/configd_validate1.cli"/>
          </leaf>
          <leaf name="in">
            <type name="string">
              <pattern value="[!-~]{1,64}">
                <error-message>
                  <value>access-list name must be 64 characters or less</value>
                </error-message>
              </pattern>
              <pattern value="[^-].*">
                <error-message>
                  <value>access-list name cannot start with "-"</value>
                </error-message>
              </pattern>
              <pattern value="[^\|;&amp;$&lt;&gt;]*">
                <error-message>
                  <value>access-list name cannot contain shell punctuation</value>
                </error-message>
              </pattern>
            </type>
            <configd:help text="Access list to apply to input packets"/>
            <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/in/configd_update.sh"/>
            <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/in/configd_delete.sh"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/access-list/in/configd_validate1.cli"/>
          </leaf>
        </container>
        <list name="interface">
          <configd:help text="Apply filtering to an interface"/>
          <configd:allowed executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/configd_allowed.sh"/>
          <key value="tagnode"/>
          <leaf name="tagnode">
            <type name="string">
              <configd:syntax executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/configd_syntax1.cli"/>
            </type>
            <configd:help text="Apply filtering to an interface"/>
            <configd:allowed executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/configd_allowed.sh"/>
          </leaf>
          <container name="access-list">
            <presence value="true"/>
            <configd:help text="Access list"/>
            <leaf name="out">
              <type name="uint32"/>
              <configd:help text="Access list to apply to output packets"/>
              <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/out/configd_update.sh"/>
              <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/out/configd_delete.sh"/>
              <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/out/configd_validate1.cli"/>
            </leaf>
            <leaf name="in">
              <type name="uint32"/>
              <configd:help text="Access list to apply to input packets"/>
              <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/in/configd_update.sh"/>
              <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/in/configd_delete.sh"/>
              <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/access-list/in/configd_validate1.cli"/>
            </leaf>
          </container>
          <container name="prefix-list">
            <presence value="true"/>
            <configd:help text="Prefix-list"/>
            <leaf name="out">
              <type name="string"/>
              <configd:help text="Prefix-list to apply to output packets"/>
              <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/out/configd_update.sh"/>
              <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/out/configd_delete.sh"/>
              <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/out/configd_validate1.cli"/>
            </leaf>
            <leaf name="in">
              <type name="string"/>
              <configd:help text="Prefix-list to apply to input packets"/>
              <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/in/configd_update.sh"/>
              <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/in/configd_delete.sh"/>
              <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/interface/prefix-list/in/configd_validate1.cli"/>
            </leaf>
          </container>
        </list>
        <container name="prefix-list">
          <presence value="true"/>
          <configd:help text="Prefix-list"/>
          <leaf name="out">
            <type name="string"/>
            <configd:help text="Prefix-list to apply to output packets"/>
            <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/out/configd_update.sh"/>
            <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/out/configd_delete.sh"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/out/configd_validate1.cli"/>
          </leaf>
          <leaf name="in">
            <type name="string"/>
            <configd:help text="Prefix-list to apply to input packets"/>
            <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/in/configd_update.sh"/>
            <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/in/configd_delete.sh"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/distribute-list/prefix-list/in/configd_validate1.cli"/>
          </leaf>
        </container>
      </container>
      <leaf-list name="passive-interface">
        <type name="string">
          <configd:syntax executable="/opt/vyatta/share/tmplscripts/protocols/ripng/passive-interface/configd_syntax1.cli"/>
        </type>
        <ordered-by value="user"/>
        <configd:help text="Suppress routing updates on an interface"/>
        <configd:allowed executable="/opt/vyatta/share/tmplscripts/protocols/ripng/passive-interface/configd_allowed.sh"/>
        <configd:create executable="/opt/vyatta/share/tmplscripts/protocols/ripng/passive-interface/configd_create.sh"/>
        <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/passive-interface/configd_delete.sh"/>
      </leaf-list>
      <leaf name="default-metric">
        <type name="uint32">
          <range value="1..16">
            <error-message>
              <value>Must be between 1-16</value>
            </error-message>
          </range>
        </type>
        <configd:help text="Metric of redistributed routes"/>
        <configd:update executable="/opt/vyatta/share/tmplscripts/protocols/ripng/default-metric/configd_update.sh"/>
        <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/default-metric/configd_delete.sh"/>
      </leaf>
      <container name="redistribute">
        <presence value="true"/>
        <configd:help text="Redistribute information from another routing protocol"/>
        <container name="bgp">
          <presence value="true"/>
          <configd:help text="Redistribute BGP routes"/>
          <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_end.sh"/>
          <leaf name="route-map">
            <type name="string"/>
            <configd:help text="Route map reference"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_routemap_validate.cli"/>
          </leaf>
          <leaf name="metric">
            <type name="uint32">
              <range value="1..16">
                <error-message>
                  <value>metric must be between 1 and 16</value>
                </error-message>
              </range>
            </type>
            <configd:help text="Metric for redistributed routes"/>
          </leaf>
        </container>
        <container name="kernel">
          <presence value="true"/>
          <configd:help text="Redistribute kernel routes"/>
          <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_end.sh"/>
          <leaf name="route-map">
            <type name="string"/>
            <configd:help text="Route map reference"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_routemap_validate.cli"/>
          </leaf>
          <leaf name="metric">
            <type name="uint32">
              <range value="1..16">
                <error-message>
                  <value>metric must be between 1 and 16</value>
                </error-message>
              </range>
            </type>
            <configd:help text="Metric for redistributed routes"/>
          </leaf>
        </container>
        <container name="connected">
          <presence value="true"/>
          <configd:help text="Redistribute connected routes"/>
          <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_end.sh"/>
          <leaf name="route-map">
            <type name="string"/>
            <configd:help text="Route map reference"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_routemap_validate.cli"/>
          </leaf>
          <leaf name="metric">
            <type name="uint32">
              <range value="1..16">
                <error-message>
                  <value>metric must be between 1 and 16</value>
                </error-message>
              </range>
            </type>
            <configd:help text="Metric for redistributed routes"/>
          </leaf>
        </container>
        <container name="static">
          <presence value="true"/>
          <configd:help text="Redistribute static routes"/>
          <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_end.sh"/>
          <leaf name="route-map">
            <type name="string"/>
            <configd:help text="Route map reference"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_routemap_validate.cli"/>
          </leaf>
          <leaf name="metric">
            <type name="uint32">
              <range value="1..16">
                <error-message>
                  <value>metric must be between 1 and 16</value>
                </error-message>
              </range>
            </type>
            <configd:help text="Metric for redistributed routes"/>
          </leaf>
        </container>
        <container name="ospfv3">
          <presence value="true"/>
          <configd:help text="Redistribute OSPFv3 routes"/>
          <configd:end executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_end.sh"/>
          <leaf name="route-map">
            <type name="string"/>
            <configd:help text="Route map reference"/>
            <configd:validate executable="/opt/vyatta/share/tmplscripts/protocols/ripng/redistribute/configd_redist_routemap_validate.cli"/>
          </leaf>
          <leaf name="metric">
            <type name="uint32">
              <range value="1..16">
                <error-message>
                  <value>metric must be between 1 and 16</value>
                </error-message>
              </range>
            </type>
            <configd:help text="Metric for redistributed routes"/>
          </leaf>
        </container>
      </container>
      <container name="default-information">
        <presence value="true"/>
        <configd:help text="Control distribution of default route"/>
        <leaf name="originate">
          <type name="empty"/>
          <configd:help text="Distribute a default route"/>
          <configd:create executable="/opt/vyatta/share/tmplscripts/protocols/ripng/default-information/originate/configd_create.sh"/>
          <configd:delete executable="/opt/vyatta/share/tmplscripts/protocols/ripng/default-information/originate/configd_delete.sh"/>
        </leaf>
      </container>
    </container>
  </augment>
  <augment target-node="/if:interfaces/interfaces-bridge:bridge/interfaces-bridge:ipv6">
    <container name="ripng">
      <presence value="true"/>
      <configd:help text="Routing Information Protocol (RIPng)"/>
      <container name="split-horizon">
        <presence value="true"/>
        <configd:help text="Split horizon parameters"/>
        <leaf name="poison-reverse">
          <type name="empty"/>
          <configd:help text="Enable poison reverse for split-horizon"/>
          <configd:create executable="/opt/vyatta/share/tmplscripts/interfaces/bridge/ipv6/ripng/split-horizon/poison-reverse/configd_create.sh"/>
          <configd:delete executable="/opt/vyatta/share/tmplscripts/interfaces/bridge/ipv6/ripng/split-horizon/poison-reverse/configd_delete.sh"/>
          <configd:validate executable="/opt/vyatta/share/tmplscripts/interfaces/bridge/ipv6/ripng/split-horizon/poison-reverse/configd_validate1.cli"/>
        </leaf>
        <leaf name="disable">
          <type name="empty"/>
          <configd:help text="Disable split horizon"/>
          <configd:create executable="/opt/vyatta/share/tmplscripts/interfaces/bridge/ipv6/ripng/split-horizon/disable/configd_create.sh"/>
          <configd:delete executable="/opt/vyatta/share/tmplscripts/interfaces/bridge/ipv6/ripng/split-horizon/disable/configd_delete.sh"/>
