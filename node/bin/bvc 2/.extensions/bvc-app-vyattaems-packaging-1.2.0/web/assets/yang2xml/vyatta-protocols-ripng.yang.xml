  <augment target-node="/if:interfaces/interfaces-dataplane:dataplane/interfaces-dataplane:vif/interfaces-dataplane:ipv6">
    <container name="ripng">
      <presence value="true"/>
      <configd:help text="Routing Information Protocol (RIPng)"/>
      <container name="split-horizon">
        <presence value="true"/>
        <configd:help text="Split horizon parameters"/>
        <leaf name="poison-reverse">
